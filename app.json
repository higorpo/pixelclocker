{
  "expo": {
    "name": "PixelClocker",
    "slug": "PixelClocker",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon@3x.png",
    "splash": {
      "image": "./assets/splash@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#82327E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.ufsc.pixel.pixelclocker",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "br.ufsc.pixel.pixelclocker",
      "versionCode": 1
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000000",
      "hidden": false,
      "translucent": true
    }
  }
}